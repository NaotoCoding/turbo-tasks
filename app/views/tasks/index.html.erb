<%= search_form_for @q, url: tasks_url do |f| %>
  <%= f.search_field :title_cont %>
  <%= f.submit %>
<% end %>

<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= render "shared/form_error", f:, field: :title %>
  <%= f.submit %>
<% end %>

<%= render partial: "task", collection: @tasks %>
