    "bootstrap-icons": "^1.11.3",
    "nodemon": "^3.1.3",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "sass": "^1.77.4"
  },
  "browserslist": [
    "defaults"
  ]
}